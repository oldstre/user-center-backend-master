<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wry.usercenter.mapper.ComponentMapper">

    <resultMap id="BaseResultMap" type="com.wry.usercenter.model.domain.Component">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="componentName" column="componentName" jdbcType="VARCHAR"/>
            <result property="componentImg" column="componentImg" jdbcType="VARCHAR"/>
            <result property="componentQuantity" column="componentQuantity" jdbcType="INTEGER"/>
            <result property="isDelete" column="isDelete" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,componentName,componentImg,
        componentQuantity,isDelete
    </sql>
</mapper>
